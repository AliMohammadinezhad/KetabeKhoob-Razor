@model string

<div class="form-element-row">
    <label asp-for="@Model" for="phone-number" class="label-element"></label>
    <div class="form-element-with-icon">
        <input asp-for="@Model" type="password" class="input-element" id="phone-number">
        <i class="fad fa-key-skeleton"></i>
    </div>
    <span asp-validation-for="@Model"></span>

</div>